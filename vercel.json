{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/node_modules/(.*)", "dest": "/node_modules/$1" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}